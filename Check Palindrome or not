#include <stdio.h>
using namespace std;
int main() {
    int n, rev=0;
    cout<<"Enter any number:";
    cin>>n;
    int temp=n;
    while(n!=0){
        int rem = n%10;
        rev=rev*10+rem;
        n=n/10;
    }
    if(temp==rev)
        cout<<"Palindromic number.";
    else
        cout<<"Not.";
     return 0;
}
