#include <bits/stdc++.h>
using namespace std;

int main() {
    char str[] = "This is a program.";
    int len = strlen(str);
    for (int i = 0; i < len; i++) {
        if (str[i] == ' ') {
            if (i > 0 && str[i-1] == ' ') {
                continue;
            }
            cout << str[i];
        }
    }
    cout << endl;
    return 0;
}
