#include <iostream>
#include <cmath>
using namespace std;

int main() {
    double num1, num2, diff, sqrt_diff;

    // Get the two numbers from the user
    cout << "Enter the first number: ";
    cin >> num1;
    cout << "Enter the second number: ";
    cin >> num2;

    // Calculate the difference
    diff = abs(num1 - num2);

    // Calculate the square root of the difference
    sqrt_diff = sqrt(diff);

    // Output the result
    cout << "The square root of the difference between " << num1 << " and " << num2 << " is: " << sqrt_diff << endl;

    return 0;
}
